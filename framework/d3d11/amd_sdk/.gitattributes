# This is a windows-only project. Force CRLF.
* text eol=crlf

# Explicit settings for specific file types.
*.h         eol=crlf
*.cpp       eol=crlf
*.inl       eol=crlf
*.hlsl      eol=crlf
*.rc        eol=crlf
*.manifest  eol=crlf
*.vcxproj   eol=crlf
*.filters   eol=crlf
*.props     eol=crlf
*.sln       eol=crlf
*.bat       eol=crlf
*.txt       eol=crlf
*.lua       eol=crlf
*.md        eol=crlf
*.pdf       binary
*.ppsx      binary
*.ico       binary
*.png       binary
*.dds       binary
*.sdkmesh   binary
*.dll       binary
*.exe       binary
